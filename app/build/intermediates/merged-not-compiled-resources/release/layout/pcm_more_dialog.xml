<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:measureMargin="true"
    app:measureWith="none"
    app:resizeChildren="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="633px"
        android:layout_height="416px"
        android:layout_centerInParent="true"
        android:background="@drawable/manage_config_popup">

        <TextView
            android:id="@+id/editName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:fontFamily="@font/gilroy_medium"
            android:gravity="center"
            android:lines="2"
            android:text="@string/ac77"
            android:textColor="@drawable/txt_selector"
            android:textSize="@dimen/_13sdp"
            app:layout_constraintBottom_toTopOf="@+id/delete"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="514px"
            android:layout_height="3px"
            android:src="@drawable/manage_config_divider"
            app:layout_constraintBottom_toTopOf="@+id/delete"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editName" />

        <TextView
            android:id="@+id/delete"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:fontFamily="@font/gilroy_medium"
            android:gravity="center"
            android:lines="2"
            android:text="@string/ac78"
            android:textColor="@drawable/txt_selector"
            android:textSize="@dimen/_12sdp"
            app:layout_constraintBottom_toTopOf="@+id/export"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editName" />

        <ImageView
            android:layout_width="514px"
            android:layout_height="3px"
            android:src="@drawable/manage_config_divider"
            app:layout_constraintBottom_toTopOf="@+id/export"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/delete" />

        <TextView
            android:id="@+id/export"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:fontFamily="@font/gilroy_medium"
            android:gravity="center"
            android:lines="2"
            android:text="@string/ac79"
            android:textColor="@drawable/txt_selector"
            android:textSize="@dimen/_13sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/delete" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>
