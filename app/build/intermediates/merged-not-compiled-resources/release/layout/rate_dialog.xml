<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mainCl"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:measureMargin="true"
    app:measureWith="none"
    app:resizeChildren="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="906px"
        android:layout_height="860px"
        android:background="@drawable/rate_popup">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline2"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.15" />

        <ImageView
            android:id="@+id/cancel"
            android:layout_width="62px"
            android:layout_height="62px"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_20sdp"
            android:src="@drawable/effect_cancel"
            app:layout_constraintBottom_toTopOf="@+id/guideline3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline2" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.33" />

        <LinearLayout
            android:id="@+id/linearLayout6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cancel">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:fontFamily="@font/gilroy_semi_bold"
                android:gravity="center"
                android:lines="2"
                android:text="@string/ac94"
                android:textColor="@color/black"
                android:textSize="@dimen/_18sdp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_15sdp"
                android:layout_marginLeft="@dimen/_15sdp"
                android:layout_marginRight="@dimen/_15sdp"
                android:fontFamily="@font/gilroy_regular"
                android:gravity="center"
                android:lines="1"
                android:text="@string/ac95"
                android:textColor="@color/black"
                android:textSize="@dimen/_13sdp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/rate"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout6">

            <ImageView
                android:id="@+id/star1"
                android:layout_width="70px"
                android:layout_height="70px"
                android:src="@drawable/star_unfill" />

            <ImageView
                android:id="@+id/star2"
                android:layout_width="70px"
                android:layout_height="70px"
                android:layout_marginHorizontal="@dimen/_6sdp"
                android:layout_marginLeft="@dimen/_6sdp"
                android:layout_marginRight="@dimen/_6sdp"
                android:src="@drawable/star_unfill" />

            <ImageView
                android:id="@+id/star3"
                android:layout_width="70px"
                android:layout_height="70px"
                android:src="@drawable/star_unfill" />

            <ImageView
                android:id="@+id/star4"
                android:layout_width="70px"
                android:layout_height="70px"
                android:layout_marginHorizontal="@dimen/_6sdp"
                android:layout_marginLeft="@dimen/_6sdp"
                android:layout_marginRight="@dimen/_6sdp"
                android:src="@drawable/star_unfill" />

            <ImageView
                android:id="@+id/star5"
                android:layout_width="70px"
                android:layout_height="70px"
                android:src="@drawable/star_unfill" />
        </LinearLayout>

        <TextView
            android:id="@+id/rate"
            android:layout_width="419px"
            android:layout_height="122px"
            android:background="@drawable/effect_rate_us_now"
            android:fontFamily="@font/gilroy_semi_bold"
            android:gravity="center"
            android:lines="1"
            android:text="@string/ac96"
            android:textColor="@color/white"
            android:textSize="@dimen/_15sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>
